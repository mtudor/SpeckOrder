        "zf2",
        "speckcommerce"
    ],
    "homepage": "http://github.com/speckcommerce/SpeckOrder",
    "authors": [
        {
            "name": "Aleksey Khudyakov",
            "email": "aleksey@xerkus.pro",
            "homepage": ""
        },
        {
            "name": "Nigel Lundsten",
